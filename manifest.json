{
    "name": "Slack-Up",
    "version": "2.0.0",
    "manifest_version": 2,
    "short_name": "slack-up",
    "description": "Level-up your slack page with DM/Channel alias + icons and 'Hide message' action button and etc",
    "icons": {
        "32": "icon/32.png",
        "45": "icon/45.png",
        "64": "icon/64.png",
        "128": "icon/128.png",
        "256": "icon/256.png"
    },
    "content_scripts": [{
        "matches": ["https://*.slack.com/*"],
        "js": ["js/app.min.js"]
    }]
}