{
    "name": "Slack-Up",
    "version": "1.4.0",
    "manifest_version": 2,
    "short_name": "slack-up",
    "description": "Level-up your slack page with DM/Channel alias + icons and 'Hide message' action button",
    "icons": {
        "128": "icon/128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.slack.com/*"
            ],
            "js": [
                "js/app.min.js"
            ]
        }
    ]
}