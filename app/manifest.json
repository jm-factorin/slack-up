{
    "name": "Slack-Up",
    "version": "1.2.1",
    "manifest_version": 2,
    "short_name": "slack-up",
    "description": "Level-up your slack page with @mention + 'Hide for me' buttons and DM and Channel alias + icons",
    "update_url": "https://raw.githubusercontent.com/jm-factorin/slack-up/master/updates.xml",
    "icons": {
        "128": "icon/128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.slack.com/*"
            ],
            "js": [
                "js/app.min.js"
            ]
        }
    ]
}